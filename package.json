{
  "name": "sound-waves",
  "version": "1.0.0-dev.3",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/sound-waves.git"
  },
  "devDependencies": {
    "grunt": "~1.1.0"
  },
  "phet": {
    "requirejsNamespace": "SOUND_WAVES",
    "supportsSound": true,
    "simulation": true,
    "runnable": true,
    "readmeCreatedManually": true,
    "supportedBrands": [
      "phet",
      "adapted-from-phet"
    ],
    "simFeatures": {
      "supportsSound": true,
      "supportsDynamicLocale": true
    },
    "screenNameKeys": [
      "SOUND_WAVES/singleSource.title",
      "SOUND_WAVES/measure.title",
      "SOUND_WAVES/twoSource.title",
      "SOUND_WAVES/reflection.title",
      "SOUND_WAVES/airPressure.title"
    ]
  },
  "eslintConfig": {
    "extends": "../chipper/eslint/sim_eslintrc.js",
    "rules": {
      "todo-should-have-issue": "off"
    }
  }
}