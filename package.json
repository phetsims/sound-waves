{
  "name": "sound-waves",
  "version": "1.2.0-dev.0",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/sound-waves.git"
  },
  "devDependencies": {
    "grunt": "~1.1.0"
  },
  "phet": {
    "requirejsNamespace": "SOUND_WAVES",
    "simulation": true,
    "runnable": true,
    "readmeCreatedManually": true,
    "supportedBrands": [
      "phet",
      "adapted-from-phet"
    ],
    "simFeatures": {
      "supportsSound": true,
      "supportsDynamicLocale": true
    },
    "screenNameKeys": [
      "SOUND_WAVES/screen.singleSource",
      "SOUND_WAVES/screen.measure",
      "SOUND_WAVES/screen.twoSource",
      "SOUND_WAVES/screen.reflection",
      "SOUND_WAVES/screen.airPressure"
    ],
    "published": true
  },
  "eslintConfig": {
    "extends": "../chipper/eslint/sim_eslintrc.js"
  }
}